{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ApplicationConnection": "Host=localhost;Port=5432;Database=CustomCADs;Username=postgres;Password=<your_password>"
  },
  "JwtSettings": {
    "Issuer": "localhost",
    "Audience": "localhost",
    "SecretKey": "<Your_Secret_Key>"
  },
  "Payment": {
    "SecretKey": "<Your_Secret_Key>",
    "PublishableKey": "<Your_Publishable_Key>",
    "TestSecretKey": "<Your_Test_Secret_Key>",
    "TestPublishableKey": "<Your_Test_Publishable_Key>"
  },
  "Email": {
    "Server": "<Your_Server>",
    "Port": 587,
    "From": "<Your_Email>",
    "Password": "<Your_Password>"
  },
  "Storage": {
    "Region": "us-east-1",
    "BucketName": "customcads-bucket",
    "AccessKey": "<Your_Access_Key>",
    "SecretKey": "<Your_Secret_Key>"
  },
  "Delivery": {
    "Username": "<Your_Username>",
    "Password": "<Your_Password>",
    "TestUsername": "<Your_Test_Username>",
    "TestPassword": "<Your_Test_Password>"
  },
  "URLs": {
    "Client": "https://localhost:5173"
  }
}
